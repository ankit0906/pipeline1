pipeline {
    agent any 
    stages {
        stage('hello') { 
            steps {
                echo "hello world"
                
            }
        }
        stage('Test') { 
            steps {
                echo "bye"
                 
            }
        }
        stage('Deploy') { 
            steps {
                echo "deployed successfully"
                 
            }
        }
    }
}
